<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="default" extends="struts-default">
		<action name="AddArticleAction" class="com.example.AddArticleAction"
			method="post">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="UpdateArticleAction" class="com.example.UpdateArticleAction"
			method="post">
			<result name="success">/displayArticles.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="DeleteArticleAction" class="com.example.DeleteArticleAction"
			method="post">
			<result name="success">/displayArticles.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="printAction" class="com.example.printAction"
			method="post">
			<result name="success">/index.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		
	</package>
</struts>